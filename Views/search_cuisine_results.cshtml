@{
    Layout="Shared/_Layout.cshtml";
}

@if (@Model.Count == 0)
{
  <p>We didn't find any cuisines matching your search!</p>
}
else
{
  <h1>Here is a list of matching cuisines:</h1>
  <ul>
    @foreach (var cuisine in Model)
    {
      <a href="/cuisine/@Model["cuisine"].GetId()"><li>@cuisine.GetName()</li></a>
    }
  </ul>
}
<p><a href="/">Return home</a></p>
