@{
    Layout="Shared/_Layout.cshtml";
}
<h1>All cuisines</h1>
@if (@Model.Count == 0)
{
  <p>There are no cuisines yet.</p>
}
<ul>
  @foreach (var cuisine in Model)
  {
    <li><a href="/cuisines/@cuisine.GetId()">@cuisine.GetName()</a></li>
  }
</ul>
<a href="/cuisines/new">Add new cuisine</a>
  <p><a href="/" class="links">Go back home</a></p>
